<Window x:Class="Monopoly.Spotify"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Monopoly"
        xmlns:eo="http://schemas.essentialobjects.com/wpf/"
        mc:Ignorable="d"
        Title="Spotify" Height="450" Width="800">
    <Grid>
        <eo:WebControl x:Name="Web">
            <eo:WebControl.WebView>
                <eo:WebView Url="https://accounts.spotify.com/pl/login?continue=https:%2F%2Fopen.spotify.com%2F">
                </eo:WebView>
            </eo:WebControl.WebView>
        </eo:WebControl>
    </Grid>
</Window>
